{
  "name": "valepro-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "start:mock": "json-server --watch lookandfeel.json --port 3001",
    "admin": "ng s --project=Administrator",
    "auth": "ng s --project=Authorizer",
    "all": "npm-run-all --parallel admin auth",
    "admindevelop": "ng build Administrator --configuration=develop",
    "adminstage": "ng build Administrator --configuration=stage",
    "adminprod": "ng build Administrator --configuration=production",
    "authdevelop": "ng build Authorizer --configuration=develop",
    "authstage": "ng build Authorizer --configuration=stage",
    "authprod": "ng build Authorizer --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^16.0.4",
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.3",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.5",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@ngrx/store": "^16.2.0",
    "angular-code-input": "^2.0.0",
    "bootstrap": "^5.3.1",
    "ng-otp-input": "^1.9.3",
    "ng-recaptcha": "^12.0.2",
    "rxjs": "~7.8.0",
    "swiper": "^10.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "json-server": "^0.17.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.2.0",
    "ngx-build-plus": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.3"
  }
}